    <div class="py-5 px-5 announce container-fluid">
      <div class="row px-5">
        <div class="col-sm-6">
          <div class="card card-flip mb-3 border-primary" style="width: 80%">
            <div class="card-front">
              <div class="card-header card-title text-center">
                <strong>GENERAL ANNOUNCEMENTS</strong>
                <div class="postcard__bar"></div>
              </div>
              <div class="card-body">
                <div class="card-text">
                  <% for(let announcement of announcementsALL1){ %>
                    <a href="#">
                      <strong>XX XXX XXXX :- </strong><%=announcement.description%>
                    </a>
                    <br /><br />
                    <% } %>
                </div>
              </div>
            </div>
