    <div class="limiter">
      <div class="container-login100">
        <div class="wrap-login100">
          <div class="login100-pic js-tilt" data-tilt>
            <img src="/users/images/img-01.png" alt="IMG">
          </div>
  
          <form action="/register_<%=role%>" method="POST" class="login100-form validate-form" novalidate>
            <span class="login100-form-title">
              Sign Up
            </span>
  
            <div class="wrap-input100 validate-input" data-validate = "Username Should not be empty">
              <input class="input100" type="text" id="Username" name="Username" placeholder="Username" required autofocus>
              <span class="focus-input100"></span>
              <span class="symbol-input100">
                <i class="icon-user"></i>
              </span>
            </div>
            <div class="wrap-input100 validate-input" data-validate = "branch Should not be empty">
              <input class="input100" type="text" id="branch" name="branch" placeholder="branch" required autofocus>
              <span class="focus-input100"></span>
              <span class="symbol-input100">
                <i class="icon-btc"></i>
              </span>
            </div>
  
                      <div class="wrap-input100 validate-input" data-validate = "Valid Student email is required: ex@student.nitw.ac.in">
              <input class="input100" type="email" id="email" name="email" placeholder="Email" required>
              <span class="focus-input100"></span>
              <span class="symbol-input100">
                <i class="fa fa-envelope" aria-hidden="true"></i>
              </span>
            </div>
  
            <div class="wrap-input100 validate-input" data-validate = "Password is required">
              <input class="input100" type="password" name="password" id="password" placeholder="Password" required>
              <span class="focus-input100"></span>
              <span class="symbol-input100">
                <i class="fa fa-lock" aria-hidden="true"></i>
              </span>
            </div>
            
            <div class="container-login100-form-btn">
              <button class="login100-form-btn">
                Sign Up
              </button>
            </div>

            
            <div class="text-center p-t-60">

              <% if(role=='faculty') { %>
              <a class="txt2" href='/login_faculty'>
                Sign IN
                <i class="fa fa-long-arrow-right m-l-5" aria-hidden="true"></i>
              </a>
              <% } else {%>
                <a class="txt2" href='/login_student'>
                  Sign IN
                  <i class="fa fa-long-arrow-right m-l-5" aria-hidden="true"></i>
                </a>
                <% }%>
              <a style="margin-left:50px" class="txt2" href="/">
                Home
                <i class="fa fa-long-arrow-right m-l-5" aria-hidden="true"></i>
              </a>
            </div>
          </form>
        </div>
      </div>
    </div>

